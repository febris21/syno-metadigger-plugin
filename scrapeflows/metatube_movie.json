{
  "type": "movie",
  "site": "metadigger.zhanghao.org.cn",
  "steps": [
    {
      "doh": {
        "host": "metadigger.zhanghao.org.cn"
      }
    },
    {
      "http": {
        "url": "https://metadigger.zhanghao.org.cn/v1/movies/search/{title}",
        "method": "GET",
        "headers": {
          "Host": "metadigger.zhanghao.org.cn",
          "User-Agent": "Mozilla/5.0 (Linux; Android 6.0; Nexus 5 Build/MRA58N) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/115.0.0.0 Mobile Safari/537.36"
        },
        "result": "metadata"
      }
    },
    {
      "collect": {
        "source": "metadata",
        "into": {
          "data_array": "['ex_digger', '']"
        }
      }
    },
    {
      "retval": {
        "source": "data_array"
      }
    }
  ]
}